{
    "version": 2,
    "builds": [
        {
            "src": "/src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api-docs",
            "dest": "/src/app.js"
        },
        {
            "src": "/api-docs/(.*)",
            "dest": "/src/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "/src/index.js"
        }
    ]
}